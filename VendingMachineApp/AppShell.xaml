<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="VendingMachineApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:VendingMachineApp"
    xmlns:views="clr-namespace:VendingMachineApp.Vistas"
    Title="VendingMachineApp">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />


    <ShellContent
        Title="Machine"
        ContentTemplate="{DataTemplate views:VendingMachine}"
        Route="VendingMachine" />

</Shell>
